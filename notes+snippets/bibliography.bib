@article{OuattaraLemasson2009:Campbells-Monke,
	Abstract = {
<p>Human language has evolved on a biological substrate with phylogenetic roots deep in the primate lineage. Here, we describe a functional analogy to a common morphological process in human speech, affixation, in the alarm calls of free-ranging adult Campbell's monkeys (<italic>Cercopithecus campbelli campbelli</italic>). We found that male alarm calls are composed of an acoustically variable stem, which can be followed by an acoustically invariable suffix. Using long-term observations and predator simulation experiments, we show that suffixation in this species functions to broaden the calls' meaning by transforming a highly specific eagle alarm to a general arboreal disturbance call or by transforming a highly specific leopard alarm call to a general alert call. We concluded that, when referring to specific external events, non-human primates can generate meaningful acoustic variation during call production that is functionally equivalent to suffixation in human language.</p>
},
	Author = {Ouattara, Karim AND Lemasson, Alban AND Zuberb\"{u}hler, Klaus},
	Date-Added = {2013-03-14 10:15:05 +0000},
	Date-Modified = {2013-03-14 15:55:51 +0000},
	Doi = {10.1371/journal.pone.0007808},
	Journal = {PLoS ONE},
	Month = {11},
	Number = {11},
	Pages = {e7808},
	Publisher = {Public Library of Science},
	Title = {Campbell's Monkeys Use Affixation to Alter Call Meaning},
	Url = {http://dx.doi.org/10.1371%2Fjournal.pone.0007808},
	Volume = {4},
	Year = {2009},
